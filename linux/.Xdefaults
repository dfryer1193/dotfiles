!*faceName: Meslo 
!*faceSize: 10

Xft.autohint:   0
Xft.hintstyle:  hintfull
Xft.lcdfilter:  lcddefault
Xft.hinting:    1
Xft.antialias:  1
Xft.rgba:       rgb

!URxvt.letterSpace:  2

URxvt.perl-ext-common:      default,clipboard,url-select,key
URxvt.url-select.launcher:  /usr/bin/google-chrome-unstable
URxvt.urlLauncher:          /usr/bin/google-chrome-unstable
URxvt.matcher.button:       1
URxvt.url-select.underline: true

URxvt.keysym.C-u:           perl:url-select:select_next
URxvt.keysym.C-g:           perl:keyboard-select:activate
URxvt.keysym.C-s:           perl:keyboard-select:search

URxvt.keysym.Control-Up:    \033[1;5A
URxvt.keysym.Control-Down:  \033[1;5B
URxvt.keysym.Control-Left:  \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C 

URxvt.depth:        32
URxvt.scrollBar:    false
URxvt*.background:  [70]#161616
URxvt*.foreground:  #cccccc
URxvt*.cursorColor: #cccccc

! black
*.color0:       #222222
*.color8:       #666666

! red
*.color1:       #e84f4f
*.color9:       #d23d3d

! green
*.color2:       #b7ce42
*.color10:      #bde077

! yellow
*.color3:       #fea63c
*.color11:      #ffe863

! blue
*.color4:       #66aabb
*.color12:      #aaccbb

! magenta
*.color5:       #b7416e
*.color13:      #e16a98

! cyan
*.color6:       #6d878d
*.color14:      #42717b

! white
*.color7:       #dddddd
*.color15:      #cccccc

! underline when default
URxvt*.colorUL: #dddddd
